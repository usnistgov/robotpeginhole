// A launch configuration that compiles the extension and then opens it inside a new window
{
    "version": "0.1.0",
    "configurations": [
      {
        "name": "(Windows) Attach",
        "type": "cppvsdbg",
        "request": "attach",
        "processId": "${command:pickProcess}"
      },
      {
      "name": "ROS: Attach",
      "type": "ros",
      "request": "attach"
    }
  ]  
}